@model ProjetoSaude.Domain.DTO.AgendaDTO
<h1>Delete</h1>

<h4>Agenda</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Delete">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="id" />
            <input type="hidden" asp-for="titulo" />
            <input type="hidden" asp-for="descricao" />
            <input type="hidden" asp-for="dataConsulta" />
            <input type="hidden" asp-for="pacienteId" />

            <div class="form-group">
                <label asp-for="titulo" class="control-label"></label>
                <label asp-for="titulo" class="form-control">@Model.titulo</label>
                <span asp-validation-for="titulo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="descricao" class="control-label"></label>
                <label asp-for="descricao" class="form-control">@Model.descricao</label>
                <span asp-validation-for="descricao" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="dataConsulta" class="control-label"></label>
                <label asp-for="dataConsulta" class="form-control">@Model.dataConsulta</label>
                <span asp-validation-for="dataConsulta" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="pacienteId" class="control-label"></label>
                <label asp-for="pacienteId" class="form-control">@Model.pacienteId</label>
                <span asp-validation-for="pacienteId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Delete" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
    }